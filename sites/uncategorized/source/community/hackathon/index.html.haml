  window.nextHackathonDate = nextHackathonDate;
  window.hackathonIssueID = hackathonIssueID;
:plain
  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "Hackathon",
    "name": "GitLab Hackathon",
    "description": "Get together with the GitLab community to contribute, learn, and win exciting prizes.",
    "organizer" : "GitLab",
    "startDate": "#{hackathon_start_date.strftime('%Y-%m-%d')}",
    "endDate": "#{hackathon_end_date.strftime('%Y-%m-%d')}",
    "eventStatus": "https://schema.org/EventScheduled",
    "eventAttendanceMode": "https://schema.org/OnlineEventAttendanceMode",
    "location": {
      "@type": "VirtualLocation",
      "url": "https://about.gitlab.com/community/hackathon/"
