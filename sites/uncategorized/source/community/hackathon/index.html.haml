- # Update the following 3 values only
- hackathon_start_date = Date.new(2023, 12, 11)
- meetup_event_id = "296831460"
- hackathon_issue_id = "353"

- hackathon_end_date = hackathon_start_date + 7
- hackathon_merge_date = hackathon_end_date + 31
- meetup_link = "https://www.meetup.com/gitlab-virtual-meetups/events/#{meetup_event_id}/"
