    start: new Date('#{hackathon_start_date.strftime('%B %-d, %Y')} 00:00:00 UTC').getTime(),
    end: new Date('#{hackathon_end_date.strftime('%B %-d, %Y')} 00:00:00 UTC').getTime(),
  };
